@model PTAData.Entities.Member

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Member</legend>

    <div class="display-label">Name</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.Name).ToString()</div>

    <div class="display-label">Phone</div>
    <div class="display-field">@Model.Phone</div>

    <div class="display-label">Email</div>
    <div class="display-field">@Model.Email</div>

    <div class="display-label">Membership</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.Membership).ToString()</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.MemberId }) |
    @Html.ActionLink("Back to List", "Index")
</p>


