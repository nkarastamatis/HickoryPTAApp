
<div id="sponsors-carousel" class="carousel slide" @*data-ride="carousel"*@>
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#sponsors-carousel" data-slide-to="0"></li>
        <li data-target="#sponsors-carousel" data-slide-to="1"></li>
        <li data-target="#sponsors-carousel" data-slide-to="2"></li>
        <li data-target="#sponsors-carousel" data-slide-to="3"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img class="sponsorCarouselImg" src="~/Content/files/SponsorsImages/images.jpg">
            <div class="carousel-caption">
                ...
            </div>
        </div>
        <div class="item">
            <img class="sponsorCarouselImg" src="~/Content/files/SponsorsImages/star-wars-business-cards-4.jpg">
            <div class="carousel-caption">
                ...
            </div>
        </div>
        <div class="item">
            <img class="sponsorCarouselImg" src="~/Content/files/SponsorsImages/star-wars-business-cards-8.jpg">
            <div class="carousel-caption">
                ...
            </div>
        </div>
        <div class="item">
            <img class="sponsorCarouselImg" src="~/Content/files/SponsorsImages/Business-Card.png">
            <div class="carousel-caption">
                ...
            </div>
        </div>        
    </div>

    <!-- Controls -->
    <a class="left carousel-control" href="#sponsors-carousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#sponsors-carousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
  
